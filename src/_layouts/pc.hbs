<!DOCTYPE html>
<html lang="ja" dir="ltr">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="robots" content="noindex,nofollow">
  <meta name="description" content="{{discription}}">
  {{>common_meta}}
  <title>{{title}}</title>
  <!-- stylesheet  -->
  {{>common_style}}
  <!--  Favicons  -->
  {{>common_fav}}
  <!-- common js  -->
  {{>common_js}}
</head>
<body class="{{cat}}">
<div id="w-wrapper" class="w-wrapper">
<!-- header -->
<header id="w-pageHeader" class="navbar navbar-dark navbar-expand-lg fixed-top w-pageHeader" data-scroll-header="true">
{{>common_header}}
</header>
<!-- /w-pageHeader-->
<div class="d-lg-flex flex-row-reverse pt-5" id="main-wrap">
<main id="w-main" class="w-main col-lg-10">
   {% body %}
<aside class="print-none" id="w-aside2">
<div class="row text-white no-gutters justify-content-center">
<nav aria-label="Page navigation">
<ul class="pagination justify-content-center">
<li class="page-item disabled">
<a class="page-link" href="{{prev-page-url}}" tabindex="-1" aria-disabled="false">Previous</a>
</li>
<li class="page-item">
<a class="page-link " aria-disabled="false"  href="{{next-page-url}}">Next</a>
</li>
</ul>
</nav>
</div>
<div class="container bg-dark p-3">
</div>
</aside>
</main>
{{>common_aside1}}
</div><!-- /main-wrap -->
<!-- footer -->
{{>common_footer}}
<script src="_js/_common/prism.js"></script>
</div>
<!--w-wrapper-->
</body>
</html>
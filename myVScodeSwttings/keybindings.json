[
    {
        "key": "ctrl+alt+w",
        "command": "extension.toggleWhitespacePlus"
    },
    {
        "key": "shift+alt+oem_102",
        "command": "editor.action.toggleWordWrap"
    },
    {
        "key": "alt+z",
        "command": "-editor.action.toggleWordWrap"
    }
]